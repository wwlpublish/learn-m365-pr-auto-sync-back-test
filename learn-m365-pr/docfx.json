{
  "build": {
    "content": [
      {
        "files": [
          "achievements.yml"
        ],
        "dest": "achievements/main/"
      },
      {
        "src": "m365",
        "files": [
          "**.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/includes/**"
        ],
        "dest": "modules"
      },
      {
        "src": "wwl-mba",
        "files": [
          "**.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/includes/**"
        ],
        "dest": "modules"
      },
      {
        "src": "wwl",
        "files": [
          "**.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/includes/**"
        ],
        "dest": "modules"
      },
      {
        "src": "m365-developer",
        "files": [
          "**.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/includes/**"
        ],
        "dest": "modules"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "learn-m365-pr/**",
          "wwl/**",
          "wwl-mba/**",
          "**/design/**",
          "**/resources/**"
        ]
      }
    ],
    "resource": [

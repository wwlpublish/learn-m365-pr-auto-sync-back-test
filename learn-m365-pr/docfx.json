        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/design/**",
          "**/resources/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/learn/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "ms.suite": "office",
      "clicktale": true,
      "feedback_system": "None",
      "uhfHeaderId": "MSDocsHeader-Learn"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "learn-m365-pr",
    "markdownEngineName": "markdig"
  }
}
